<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">


    <changeSet id="0001" author="ad">
        <createTable tableName="doctor">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" />
            </column>
            <column name="firstName" type="VARCHAR(100)" />
            <column name="lastName" type="VARCHAR(100)" />
            <column name="password" type="VARCHAR(100)" />
            <column name="role" type="VARCHAR(100)" />
            <column name="age" type="INT" />
            <column name="specialist" type="VARCHAR(100)" />
            <column name="experience" type="VARCHAR(200)" />
        </createTable>

    </changeSet>
</databaseChangeLog>